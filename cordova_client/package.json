    "license": "Apache-2.0",
    "dependencies": {
        "cordova-android": "^8.1.0",
        "cordova-browser": "^6.0.0"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4",
        "typescript": "^3.9.7",
        "cordova-plugin-whitelist": "1.3.4",
        "cordova-admob-plus": "0.36.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-admob-plus": {
