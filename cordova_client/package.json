{
    "name": "helloworld",
    "displayName": "MapleCloneGame",
    "version": "1.0.0",
    "description": "maplestory multiplayer clone game",
    "main": "index.js",
    "scripts": {
        "tscWatch": "tsc --build tsconfig.json -w",
        "tscBuild": "tsc --build tsconfig.json",
        "parcelWatch": "parcel -p 9999 watch ./src/index.html --open --no-cache",
        "androidTest": "cordova build android",
        "androidBuild": "cordova build android --release --buildConfig",
        "browserTest": "cordova build browser",
        "browserBuild": "cordova build browser --release",
        "http": "cd ./dist_browser/ && http-server -p 9998 -c-1"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
