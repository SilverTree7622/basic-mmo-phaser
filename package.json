  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jerenaux/basic-mmo-phaser/issues"
  },
  "homepage": "https://github.com/Jerenaux/basic-mmo-phaser#readme",
  "dependencies": {
    "express": "^4.15.2",
    "phaser": "^3.24.1",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "parcel": "^1.12.4",
    "phaser3_gui_inspector": "^1.2.0"
  }
}
