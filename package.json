{
  "name": "basic-mmo-phaser",
  "version": "1.0.0",
  "description": " Very basic multiplayer online game example made with Phaser, Node.js and Socket.io ",
  "main": "server.js",
  "scripts": {
    "server": "node ./server/server.js",
    "build": "parcel build index.html --open -p 9999",
    "watch": "parcel watch index.html --open -p 9998",
    "http": "http-server index/html -p 9997 -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerenaux/basic-mmo-phaser.git"
  },
